{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "dossier-files-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/dossierFiles/DossierFilesWebPart.js",
          "manifest": "./src/webparts/dossierFiles/DossierFilesWebPart.manifest.json"
        }
      ]
    },
    "todo-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/todo/TodoWebPart.js",
          "manifest": "./src/webparts/todo/TodoWebPart.manifest.json"
        }
      ]
    },
    "hello-world-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/helloWorld/HelloWorldWebPart.js",
          "manifest": "./src/webparts/helloWorld/HelloWorldWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "DossierFilesWebPartStrings": "lib/webparts/dossierFiles/loc/{locale}.js",
    "TodoWebPartStrings": "lib/webparts/todo/loc/{locale}.js",
    "HelloWorldWebPartStrings": "lib/webparts/helloWorld/loc/{locale}.js"
  }
}
